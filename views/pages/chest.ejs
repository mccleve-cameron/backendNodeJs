<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/home.css">
</head>
<body onload="loadWorkouts()">
    <h1>Select a Chest Workout</h1>
<div id="table"></div>
</body>
<footer class="bottom">
  <div class="icon"><p>Settings</p></div>
  <a href="/bodyMap"><div class="icon"><img src="/images/whiteBody.png" alt=""></div></a>
  <div class="icon"><p>Log</p></div>
</footer>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

<script>
    function loadWorkouts()
    {
      console.log('checkout run');
      $.ajax({
        url: '/getWorkouts',
        type: 'POST',
        dataType: 'json', //will parse json into javascript object
        //callback called when suceed
        success: (data) => {
          console.log('ajax success!', data);
             product = "";
          $.each(data, function (index, value) {
            product += "<form action='/workout' method='GET'><button class='bar' type='submit'><div><img src=../images/" + this.image_path + " class='imageIcon'></div>";
            product += "<div><h3 name='workout' value="+ this.id +">"+ this.name +"</h3></div></button></form>";
          });// END LOOP
        //added end
          result = "";
          result = product;
          //select status id element display in html
          $('#table').html(result);
        }//sucess data call
      });//ajax function call
      //CART CLICK AJAX END
    }//cartPopup on click
</script>
</html>