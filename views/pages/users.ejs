<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Users</h1>
    <button onclick="grab_users()">Query User's Information From Database</button>
    <div id="table"></div>
</body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

<script>
    function grab_users()
    {
      console.log('checkout run');
      $.ajax({
        url: '/getUsers',
        type: 'POST',
        dataType: 'json', //will parse json into javascript object
        //callback called when suceed
        success: (data) => {
          console.log('ajax success!', data);
             product = "";
             product = "<table><tr><th>ID</th><th>Name</th><th>Password</th></tr>";
          $.each(data, function (index, value) {
            product += "<tr>";
            product += "<td>"+ this.id +"</td>";
            product += "<td>"+ this.username + "</td>";
            product += "<td>"+ this.password + "</td>";
            product += "</tr>";
          });// END LOOP
            product += "</table>";
        //added end
          result = "";
          result = product;
          //select status id element display in html
          $('#table').html(result);
        }//sucess data call
      });//ajax function call
      //CART CLICK AJAX END
    }//cartPopup on click
</script>
</html>